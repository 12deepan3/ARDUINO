#include <Wire.h> 
#include <LiquidCrystal_I2C.h>

LiquidCrystal_I2C lcd(0x27,16,2);  

void setup()
{
  lcd.init();                     
  lcd.init();
  
  lcd.backlight();
}
  


void loop()
{
  lcd.setCursor(1,0);
  lcd.print("deepan");
  lcd.setCursor(1,1);
  lcd.print("chakaravarthy m");
  
  delay(5000);
  lcd.clear();
  
  lcd.setCursor(1,0);
  lcd.print("SNSCT");
  lcd.setCursor(1,1);
  lcd.print("EEE");
  
  delay(5000);
  lcd.clear();
}
